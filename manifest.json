{
  "name": "i8",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Download SVG icons from icons8.com",
  "homepage_url": "https://twitter.com/mehedih_",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Apply"
  },
  "permissions": [
    "https://icons8.com/*"
  ]
}