{
  "name": "i8",
  "version": "0.0.2",
  "manifest_version": 3,
  "description": "Download SVG icons from icons8.com",
  "homepage_url": "https://twitter.com/mehedih_",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Apply"
  },
  "host_permissions": [
    "https://icons8.com/*"
  ],
  "permissions": ["scripting"]
}